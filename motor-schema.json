  "schema": {
    "type": "object",
    "properties": {
      "powerInput": {"$ref": "port-schema.json#/schema"},
      "controPort": {"$ref": "controlPort-schema.json#/schema"},
      "maxTorque":  {
          "type": "string",
          "pattern": "^([0-9]+(.[0-9]+)?)(Nm|ft-lbs)$"
      },
      "ratedSpeed": {
        "type": "string",
        "pattern": "^([0-9]+(.[0-9]+)?)rpm$"
      }

    },
    "required": ["powerInput"]
  }
}